{
  "name": "express-mock-server",
  "version": "2.0.0",
  "description": "Mock server powered by Express.js",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "start": "nodemon lib/index.js --exec babel-node --presets es2015,stage-2",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel lib -s -D -d dist",
    "serve": "node dist/index.js",
    "test": "mocha --require babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Belsi/express-mock-server.git"
  },
  "keywords": [
    "mock",
    "server",
    "express"
  ],
  "author": "Frantisek Belsan <frantisek.belsan@gmail.com> (https://github.com/Belsi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Belsi/express-mock-server/issues"
  },
  "homepage": "https://github.com/Belsi/express-mock-server#readme",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-register": "6.26.0",
    "babel-preset-env": "1.7.0",
    "body-parser": "1.18.3",
    "colors": "1.3.1",
    "express": "4.16.3",
    "http-status-codes": "1.3.0",
    "morgan": "1.9.0",
    "nconf": "0.10.0",
    "rimraf": "2.6.2"
  },
  "devDependencies": {
    "gulp": "3.9.1",
    "gulp-shell": "0.6.5",
    "mocha": "5.2.0",
    "request": "2.88.0",
    "run-sequence": "2.2.1",
    "nodemon": "1.18.3"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": []
  }
}
