{
  "name": "express-mock-server",
  "version": "1.0.4",
  "description": "Mock server powered by Express.js",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "start": "nodemon lib/index.js --exec babel-node --presets es2015,stage-2",
